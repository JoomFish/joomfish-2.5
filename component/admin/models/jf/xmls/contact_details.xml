<?xml version="1.0" ?>
<joomfish type="contentelement">
	<name>Contact</name>
	<author>A. Kempkens</author>
	<version>2.5.0 alpha</version>
	<description>Definition for the core contact component</description>
	<copyright>2003 - 2012, Think Network GmbH, Munich</copyright>
	<reference>
		<table name="contact_details">
			<field type="referenceid" name="id" translate="0">ID</field>
			<field type="titletext" name="name" translate="1">Name</field>
			<field type="text" name="alias" translate="1" posthandler="filterName">Alias</field>
			<field type="category" name="catid" scope="com_contact" translate="1">Category</field>
			<field type="text" name="con_position" translate="1">Position</field>
			<field type="text" name="email_to" translate="1">Email Address</field>
			<field type="text" name="address" translate="1">Street Address</field>
			<field type="text" name="suburb" translate="1">Town/Suburb</field>
			<field type="text" name="state" translate="1">State/County</field>
			<field type="text" name="country" translate="1">Country</field>
			<field type="text" name="postcode" translate="1">Postcode</field>
			<field type="text" name="telephone" translate="1">Telefon</field>
			<field type="text" name="fax" translate="1">Fax</field>
			<field type="textarea" name="misc" translate="1">Miscellaneous Info</field>
			
			<field type="params" name="params" translate="1">Parameters</field>
			
			<field type="checked_out_by" name="checked_out" translate="0">Check out by</field>
			<field type="checked_out_date" name="checked_out_time" translate="0">Check out date</field>
		</table>
		<treatment>
			<!--<target>native</target>
			<tableclass>Content</tableclass>
			<publishedfield>state</publishedfield>
			<translationObjectModel>TranslationObjectContent</translationObjectModel>
			<jformModel>ContentJForm</jformModel>
			-->
		
			<form>contact</form>
			<modelName>ContactModelContact</modelName>
			<model>contact</model>
			<extension>com_contact</extension>
			<!-- TEST -->
			<!--
			<contentElement>contact</contentElement>
			<contentElementPath>administrator/components/com_contact/joomfish</contentElementPath>
			-->
			<!-- TEST END-->
		
		</treatment>
	</reference>
	<translationfilters>
			<category>catid</category>
	</translationfilters>
</joomfish>